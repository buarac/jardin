{
  "name": "app-bastouille",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "ts-node --project tsconfig.scripts.json scripts/pre-build-app.ts",
    "build": "next build",
    "clean": "rm -rf .next prisma/migrations node_modules",
    "dev": "ts-node --project tsconfig.scripts.json scripts/pre-build-app.ts && next dev",
    "format": "prettier --write .",
    "generate:client": "npx prisma generate",
    "lint": "next lint",
    "meteo:fetch": "ts-node scripts/fetchMeteo.ts",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy --env=.env.production",
    "postinstall": "prisma generate",
    "prepare": "prisma generate",
    "reset:db": "npx prisma migrate reset --force --skip-seed --env=.env",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@headlessui/react": "1.7.18",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.535.0",
    "next": "14.2.3",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sonner": "^2.0.7",
    "spatial-navigation-js": "^1.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "24.1.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "19.1.9",
    "autoprefixer": "10.4.15",
    "dotenv": "^17.2.1",
    "eslint": "8.45.0",
    "eslint-config-next": "14.2.3",
    "postcss": "8.4.24",
    "prettier": "3.0.0",
    "prisma": "^6.13.0",
    "tailwindcss": "3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
